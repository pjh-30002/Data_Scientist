   "source": [
    "from math import gcd\n",
    "\n",
    "def solution(array):\n",
    "    def lcm(x,y):\n",
    "        return x*y // gcd(x,y)\n",
    "    \n",
    "    while True:\n",
    "        array.append(lcm(array.pop(), array.pop()))\n",
    "        if len(array) == 1:\n",
    "            return array[0]"
